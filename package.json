{
  "name": "@vivienne/haiku-senseme",
  "version": "2.0.3",
  "description": "Control SenseME enabled devices from Haiku",
  "main": "dist/index.js",
  "repository": "vivienne/haiku-senseme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "babel --no-babelrc --plugins syntax-function-bind,transform-function-bind,transform-object-rest-spread -D -d docsrc src && jsdoc2md -f docsrc/*.js > API.md",
    "build": "babel src -d dist -D",
    "clean": "rm -rf dist/ && rm -rf docsrc/",
    "example": "node ./dist/example.js",
    "prepublish": "npm run build"
  },
  "author": "Tomi Blinnikka <tomi.blinnikka@bliny.net>",
  "license": "MIT",
  "dependencies": {
    "any-observable": "^0.5.1",
    "compute-polynomial": "^1.1.0",
    "debug": "^4.1.1",
    "just-compose": "^1.1.0",
    "just-remove": "^2.1.0",
    "just-values": "^1.1.0",
    "message-socket": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-function-bind": "^7.10.1",
    "@babel/plugin-syntax-function-bind": "^7.10.1",
    "@babel/preset-env": "^7.10.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-function-bind",
      "@babel/plugin-syntax-function-bind"
    ]
  }
}
